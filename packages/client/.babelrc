{
  "extends": "../../.babelrc",
  "presets": [
    "@babel/preset-react",
    ["@emotion/babel-preset-css-prop", {
      "autoLabel": true
    }]
  ],
  "plugins": [
    ["babel-plugin-module-resolver", {
      "cwd": "babelrc",
      "root": ["./src"],
      "extensions": [
        ".ts",
        ".tsx",
        ".js",
        ".jsx",
        ".jpg",
        ".png",
        ".svg",
        ".json"
      ]
    }]
  ]
}
