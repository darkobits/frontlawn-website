{
  "name": "@darkobits/inspirat",
  "description": "A new tab experience for Chrome in the style of Momentum.",
  "version": "0.5.1",
  "author": "darkobits",
  "license": "WTFPL",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darkobits/frontlawn-website.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "del dist",
    "check:styles": "tslint --project tsconfig.json --format codeFrame",
    "check:types": "tsc --noEmit",
    "prebuild": "npm run clean && run-p check:*",
    "build:server": "netlify-lambda --config webpack.config.lambda.js build ./src/lambda",
    "build:client": "webpack --mode=production",
    "build": "run-p build:*",
    "prebump": "npm run build",
    "bump": "standard-version",
    "bump:beta": "npm run bump -- --prerelease=beta",
    "postbump": "npm run build",
    "start:server": "netlify-lambda --config webpack.config.lambda.js serve ./src/lambda",
    "start:client": "webpack-dev-server --mode=development",
    "start": "concurrently 'npm run start:server' 'npm run start:client'"
  },
  "dependencies": {
    "axios": "^0.18.0",
