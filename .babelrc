{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "chrome": "60",
        "node": "8.0.0"
      },
      "useBuiltIns": "entry"
    }],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "babel-plugin-emotion",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
    ["babel-plugin-module-resolver", {
      "cwd": "babelrc",
      "root": ["./src"],
      "extensions": [
        ".ts",
        ".tsx",
        ".js",
        ".jsx",
        ".jpg",
        ".png",
        ".svg",
        ".json"
      ]
    }]
  ]
}
